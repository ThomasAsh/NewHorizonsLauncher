    {
      "name": "newhorizonslauncher",
      "version": "2.2.1",
      "productName": "New Horizons Launcher",
      "description": "Minecraft Launcher for the New Horizons server",
      "author": "ratedMojo",
      "license": "GPL-3.0",
      "homepage": "https://newhorizons.games",
