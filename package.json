        "ejs": "^3.1.10",
        "ejs-electron": "^3.0.0",
        "electron-updater": "^6.3.9",
        "fs-extra": "^11.1.1",
        "github-syntax-dark": "^0.5.0",
        "got": "^11.8.5",
        "helios-core": "~2.2.4",
        "helios-distribution-types": "^1.3.0",
        "jquery": "^3.7.1",
        "lodash.merge": "^4.6.2",
        "node-fetch": "^3.3.2",
        "semver": "^7.6.3",
        "toml": "^3.0.0"
      },
      "devDependencies": {
        "electron": "^33.2.1",
        "electron-builder": "^25.1.8",
        "eslint": "^8.57.1",
        "patch-package": "^8.0.0"
      },
      "build": {
        "asar": true,
        "asarUnpack": [
          "**\\node_modules\\helios-core\\**"
